snippet {
	{{${1:var}}}
snippet ngvr
	{{$root.${1:var}}}
snippet ngvp
	{{$parent.${1:var}}}
snippet ngc
	ng-class="{${1}}[${2}]"
snippet ngr
	<ul class="${1}">
		<li ng-repeat=" ${2:item} in ${3:itemlist}">${4}</li>
	</ul>
snippet ngt
	<div ng-include="${1:partials/my_partial}">
snippet ngi
	ng-init="${1:init_var}"
