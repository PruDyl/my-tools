<?php

/**
 * Base components for the chCmsAdminMyAccountPlugin adminMyRight module.
 * 
 * @package     chCmsAdminMyAccountPlugin
 * @subpackage  adminMyRight
 * @author      Your name here
 * @version     SVN: $Id$
 */
abstract class BaseadminMyRightsComponents extends chCmsAdminPanelComponents
{
  /**
   * entry point for Index action
   *
   * @param sfWebRequest $request the user request
   * @return 
   */
  public function executeIndex(sfWebRequest $request)
  {
    $this->grouped_credentials = AdminMyAccountTools::getCredentialListSortedByGroup(); 
    $this->contractor = $this->getUser();
  }
}
