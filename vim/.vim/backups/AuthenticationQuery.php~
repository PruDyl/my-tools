class AuthenticationQuery extends BaseAuthenticationQuery
{

  /**
   * find an authentication entry for given token and uniqueid
   *
   * @param String $token the provider returned token
   * @param String $uniqueId the provider returned Unique id
   * @return Authentication|null
   * @author Ludovic PellÃ© <ludovic_pelle@carpe-hora.com>
   **/
  public function findOneByTokenAndUniqueId($token, $uniqueId)
  {
    return $this
      ->filterByToken($token)
      ->filterByAccountIdentifier($uniqueId)
      ->findOne();
  }

} // AuthenticationQuery
