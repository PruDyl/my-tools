/**
 * basic user for tests
 */
class TestUser
{
  /**
   * check if user has credential, true if not admin_***
   *
   * @return void
   * @author Ludovic Pellé <ludovic_pelle@carpe-hora.com>
   **/
  public function hasAdminCredential($credential)
  {
    return 0 !== strpos($credentials, 'admin_');
  }
} // END OF TestUser
